{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["node"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {},
  "rules": {
    "prettier/prettier": "error",
    "node/no-unpublished-require": "off"
  },
  "parserOptions": {},
  "ignorePatterns": ["dist", "templates"]
}
